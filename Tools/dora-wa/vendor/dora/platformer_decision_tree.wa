func PlatformerDecisionTreeBehave(name: string, root: PlatformerBehaviorTree) => PlatformerDecisionTree {
	return *PlatformerDecisionTreeFrom(platformer_decision_leaf_behave(ToDoraString(name), root.GetRaw()))
