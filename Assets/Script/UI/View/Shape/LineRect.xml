<!-- params: X=0, Y=0, Width, Height, Color -->
<Line X="{ x or 0 }" Y="{ y or 0 }" Color="{ color or 0xffffff }" Opacity="{ color and Color(color).opacity or 1 }" RenderOrder="{ renderOrder or 0 }">
	<Vec2 X="0" Y="0"/>
	<Vec2 X="{ width }" Y="0"/>
	<Vec2 X="{ width }" Y="{ height }"/>
	<Vec2 X="0" Y="{ height }"/>
	<Vec2 X="0" Y="0"/>
</Line>
